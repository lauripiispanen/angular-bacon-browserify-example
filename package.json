{
  "name": "angular-bacon-browserify-example",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "browserify": {
    "transform": [ "browserify-shim" ]
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular"
    }
  },
  "dependencies": {
    "angular": "^1.3.6",
    "angular-bacon": "git://github.com/lauripiispanen/angular-bacon.git#module-apis",
    "baconjs": "^0.7.41"
  },
  "devDependencies": {
    "browserify": "^8.0.3",
    "browserify-shim": "^3.8.2"
  },
  "scripts": {
    "browserify": "browserify src/main.js -o bundle.js"
  },
